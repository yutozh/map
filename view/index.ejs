<div class="mui-content">
    <div id="tabbar-with-map" class="mui-control-content mui-active">

        <div id="map-container"></div>

        <!--地图顶部的状态栏-->
        <div id="map-top-control">
            <!--当前城市、更换城市-->
            <div class="sel_container">
                <strong id="curCity">{{currentCity}}</strong>
                [<a id="curCityText" href="javascript:void(0)">更换城市</a>]

                <!--定位状态-->
                <div id="map-top-locating">{{locating}}</div>

                <!--用户状态-->
                <div id="map-top-userCondition">当前状态：{{userCondition}}</div>
            </div>
            <div class="map_popup" id="cityList" style="display:none;">
                <div class="popup_main">
                    <div class="title">城市列表</div>
                    <div class="cityList" id="citylist_container"></div>
                    <button id="popup_close"></button>
                </div>
            </div>


        </div>

        <!--地图底部的接单按钮-->
        <div id="map-bottom-control">
            <div id="map-bottom-btnStart" v-if="showBtnStart" @click="clickBtnStart">
                <span class="mui-icon" v-bind:class="[btnStartClass]">{{btnStart}}</span>
            </div>


            <div id="map-bottom-status" v-if="showStatus" @click="clickStatus">
                <span id="map-bottom-status-span">{{bottomStatus}}</span>
            </div>


            <div id="map-bottom-btnOK" v-if="showBtnOK" @click="clickBtnOK">
                <span class="mui-icon" v-bind:class="[btnOKClass]">{{btnOK}}</span>
            </div>
        </div>

        <!--地图底部的导航-->
        <div id="map-bottom-guide" v-if="show">
            <div id="map-guide-left">
                <div id="map-guide-leftarrow" @click="leftArrowClick"></div>
                <div id="map-guide-counts"><span style="color: green;">{{currentNum +1 }}</span> / <span>{{steps.length}}</span>
                </div>
            </div>

            <div id="map-guide-changeable">
                <div id="map-guide-address">
                    <p id="map-guide-address-title" class="guide-small-title">目标地址</p>
                    <div id="map-guide-address-content">{{steps[currentNum].endAdd}}</div>
                </div>
                <div id="map-guide-type">
                    <p id="map-guide-type-title" class="guide-small-title">状态</p>
                    <div id="map-guide-type-content">{{steps[currentNum].type}}</div>
                </div>
            </div>

            <div id="map-guide-nextBtn">
                <div id="guide-detail">
                    <p id="map-guide-detail-title">详细信息</p>
                    <div>
                        <p class="map-guide-detail">联系人</p>
                        <div>{{steps[currentNum].name}}</div>
                    </div>
                    <div>
                        <p class="map-guide-detail">联系人电话</p>
                        <div>{{steps[currentNum].telephone}}</div>
                    </div>
                    <div>
                        <p class="map-guide-detail">联系人电话</p>
                        <div>{{steps[currentNum].telephone}}</div>
                    </div>
                </div>
                <span class="map-guide-icon icon1" @click="detailClick">
							<a href="#map-guide-detail" style="color: white;">查看</a>
						</span>
                <span class="map-guide-icon icon2" @click="nextClick">完成</span>
            </div>
        </div>

